{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
      {
        "command": {
          "action": "globalSummon"
        },
        "keys": "win+alt+z"
      },
      {
        "command": {
          "action": "closeTab"
        },
        "keys": "ctrl+shift+w"
      },
      {
        "command": "toggleFocusMode",
        "keys": "alt+shift+f"
      },
      {
        "command": {
          "action": "scrollUp"
        },
        "keys": "ctrl+up"
      },
      {
        "command": "togglePaneZoom",
        "keys": "alt+shift+z"
      },
      {
        "command": {
          "action": "scrollDown"
        },
        "keys": "ctrl+down"
      },
      {
        "command": "closePane",
        "keys": "alt+shift+w"
      },
      {
        "command": "restoreLastClosed",
        "keys": "ctrl+shift+t"
      },
      {
        "command": {
          "action": "copy",
          "singleLine": false
        },
        "keys": "ctrl+c"
      },
      {
        "command": {
          "action": "splitPane",
          "index": 0,
          "split": "auto"
        },
        "keys": "alt+shift+1"
      },
      {
        "command": "find",
        "keys": "ctrl+shift+f"
      },
      {
        "command": {
          "action": "splitPane",
          "index": 1,
          "split": "auto"
        },
        "keys": "alt+shift+2"
      },
      {
        "command": {
          "action": "splitPane",
          "index": 2,
          "split": "auto"
        },
        "keys": "alt+shift+3"
      },
      {
        "command": "paste",
        "keys": "ctrl+v"
      },
      {
        "command": {
          "action": "splitPane",
          "split": "auto",
          "splitMode": "duplicate"
        },
        "keys": "alt+shift+d"
      },
      {
        "command": {
          "action": "moveFocus",
          "direction": "nextInOrder"
        },
        "keys": "alt+end"
      },
      {
        "command": {
          "action": "moveFocus",
          "direction": "previousInOrder"
        },
        "keys": "alt+home"
      },
      {
        "command": "switchSelectionEndpoint",
        "keys": "alt+a"
      },
      {
        "command": "toggleBlockSelection",
        "keys": "alt+v"
      }
    ],
    "alwaysShowNotificationIcon": true,
    "confirmCloseAllTabs": false,
    "copyFormatting": "none",
    "copyOnSelect": true,
